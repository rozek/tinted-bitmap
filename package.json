{
  "name": "tinted-bitmap",
  "version": "0.1.1",
  "description": "creates a tinted copy of a given HTML image",
  "type": "module",
  "main": "./dist/tinted-bitmap.js",
  "module": "./dist/tinted-bitmap.esm.js",
  "exports": {
    "require": "./dist/tinted-bitmap.js",
    "import": "./dist/tinted-bitmap.esm.js"
  },
  "types": "./dist/tinted-bitmap.d.ts",
  "svelte": "./src/tinted-bitmap.svelte",
  "scripts": {
    "build": "rimraf dist && tsc && rollup -c rollup.config.js && mv src/*.d.ts dist && mv src/*.js dist/tinted-bitmap.esm.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rozek/tinted-bitmap.git"
  },
  "keywords": [
    "html-canvas",
    "bitmap",
    "tinting"
  ],
  "author": "Andreas Rozek <a.rozek@gmx.de> (https://www.rozek.de/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rozek/tinted-bitmap/issues"
  },
  "homepage": "https://github.com/rozek/tinted-bitmap#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.7",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "javascript-interface-library": "file:../javascript-interface-library"
  }
}
